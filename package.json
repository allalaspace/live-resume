{
  "name": "live-resume",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:en": "ng serve --configuration=en",
    "start:fr": "ng serve --configuration=fr",
    "start:pt": "ng serve --configuration=pt",
    "build": "ng build",
    "build:en": "ng build --configuration=en",
    "build:fr": "ng build --configuration=fr",
    "build:pt": "ng build --configuration=pt",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "int:extract": "ng xi18n --output-path src/locales",
    "build-locale:en": "ng build --aot --configuration production --i18n-locale=en --i18n-format=xlf --i18n-file=src/locales/messages.en.xlf --output-path=dist/en --base-href /en/",
    "build-locale:fr": "ng build --aot --configuration production --i18n-locale=fr --i18n-format=xlf --i18n-file=src/locales/messages.fr.xlf --output-path=dist/fr --base-href /fr/",
    "build-locale:pt": "ng build --aot --configuration production --i18n-locale=pt --i18n-format=xlf --i18n-file=src/locales/messages.pt.xlf --output-path=dist/pt --base-href /pt/",
    "build-locale": "npm run build-locale:en && npm run build-locale:pt && npm run build-locale:fr ",
    "deploy:gh-pages": "ng build --configuration=fr --base-href=https://allalaspace.github.io/live-resume/ && npx angular-cli-ghpages --dir=dist/live-resume/fr"
  },
  "homepage": "https://allalaspace.github.io/live-resume",
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/fire": "^17.0.1",
    "@angular/forms": "^17.3.12",
    "@angular/localize": "^17.3.12",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "@fortawesome/angular-fontawesome": "^0.10.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "compass-mixins": "^0.12.10",
    "elliptic": "^6.5.3",
    "firebase": "^10.8.0",
    "http-proxy": "^1.18.1",
    "ng-navigator-share": "^1.0.3",
    "node-forge": "^0.10.0",
    "rxjs": "~7.8.1",
    "serialize-javascript": "^4.0.0",
    "tslib": "^2.0.0",
    "websocket-extensions": "^0.1.4",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.17",
    "@angular/cli": "^17.3.17",
    "@angular/compiler-cli": "^17.3.12",
    "@angular/language-service": "^17.3.12",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^1.0.7",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~5.4.5"
  }
}
